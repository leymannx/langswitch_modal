/**
 * Implements hook_preprocess_HOOK().
 */
function langswitch_modal_preprocess_html(&$variables) {
  $variables['page']['#attached']['library'][] = 'langswitch_modal/scripts';
  $variables['page']['#attached']['library'][] = 'core/drupal.ajax';
}
